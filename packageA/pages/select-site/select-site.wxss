.main{
  top: 0;
}
.addressList{
  background-color: #fff;
  margin-top: 20rpx;
}
.addressItem{
  padding: 10rpx 0;
  font-size: 30rpx;
  border-bottom: 1px solid #e0e0e0;
  position: relative;
}
.addressDetail{
  border-left-width: 6px;
  border-left-style: solid;
  border-left-color: transparent;
  padding-left: 18rpx;
  line-height: 70rpx;
  width: 75%;
  display: inline-block;
}
.addressDetail.actived{
  border-left-color: #ffd600;
}
.updateAddress{
  right: 0;
  height: 104rpx;
  position: absolute;
  left: 75%;
  border-left: 1px solid #e0e0e0;
  background-image: url("data:image/png;base64,"); 
  background-repeat: no-repeat;
  background-position: center center;
  background-size: 44rpx;
  top: 30rpx;
}
.emptyAddress{
  position: absolute;
  top: 28%;
  left: 0;
  right: 0;
  text-align: center;
  padding-top: 200rpx;
  background-image: url("data:image/png;base64,");
  background-position: center 20rpx;
  background-size: 160rpx 170rpx;
  background-repeat: no-repeat;
  color: #999;
  font-size: 32rpx;
}
.addFooter{
  background-color: #fff;
  height: 100rpx;
  line-height: 100rpx;
  position: fixed;
  bottom: 0;
  width: 100%;
  border-top: 1px solid #e0e0e0;
  text-align: center;
}
.addAddress{
  background-color: #ffd600;
  width: 400rpx;
  text-align: center;
  position: absolute;
  top: 10rpx;
  line-height: 80rpx;
  height: 80rpx;
  border-radius: 10rpx;
  font-size: 34rpx;
  left: 20%;
}