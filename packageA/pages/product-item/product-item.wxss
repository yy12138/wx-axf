.main{
  top: 0;
}
.basicProductDetail{
  background-color: #fff;
  font-size: 46rpx;
  text-align: center;
  padding-bottom: 40rpx;
  margin-bottom: 10rpx;
}
.product-img{
  padding: 80rpx 20rpx;
}
.product-price{
  line-height: 90rpx;
  color: #f40;
  font-size: 44rpx;
}
.productDetail{
  background-color: #fff;
  font-size: 28rpx;
  line-height: 60rpx;
  margin-bottom: 10rpx;
}
.ws{
  display: inline-block;
  width: 25%;
  color: #848c99;
  text-align: center;
}
.product-title{
  text-align: center;
  color: #e0bd6a;
  font-size: 28rpx;
  height: 90rpx;
  line-height: 90rpx;
  background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAV8AAAAEBAMAAADbpSEyAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAqUExURQAAAOC9auC9auC9auC9auC9auC9auC9auC9auC9auC9auC9auC9auC9agdMhdkAAAANdFJOUwCZRGaID/C0S1EkGCCdfVLsAAAAM0lEQVQoz2NgIAWw3r3GQFXQe7eAgZZgMDjYhVhwgHYO5hQkGggwGBMLNtDOwVxKRAMFAOUAK2mKnCc7AAAAAElFTkSuQmCC");
  background-repeat: no-repeat;
  background-position: center center;
  background-size: auto 4rpx;
}
.pictureDetail{
  background-color: #fff;
  margin-bottom: 140rpx;
}
.promise{
  width: 100%;
  height: 500rpx;
  background-image: url("https://s1.ax1x.com/2018/02/05/9Kfz0x.jpg");
  background-size: 100% 100%;
}
.foot{
  background-color: #fff;
  position: fixed;
  bottom: 0;
  height: 100rpx;
  left: 0;
  right: 0;
  font-size: 28rpx;
  padding: 14rpx 0 14rpx 14rpx;
  box-sizing: border-box;
}
.favorite{
  text-align: center;
  font-size: 20rpx;
  padding-top: 36rpx;
  width: 80rpx;
  display: inline-block;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: 56% top;
  background-size: 36rpx auto;
}
.addToCart{
  display: inline-block;
  position: absolute;
  left: 120rpx;
  top: 0;
  bottom: 0;
  line-height: 100rpx;
}
.cartOperateBtn{
  display: inline-block;
}
.subCartNum,.addCartNum,.num{
  display: inline-block;
  width: 52rpx;
  height: 52rpx;
  text-align: center;
  line-height: 52rpx;
}
.subCartNum,.addCartNum{
  border: 1px solid #ff4300;
  color: #ff4300;
  font-weight: 700;
  font-size: 34rpx;
  border-radius: 50%;
  box-sizing: border-box;
}
.cartIcon{
  position: absolute;
  top: -40rpx;
  right: 20rpx;
  z-index: 4;
  width: 108rpx;
  height: 108rpx;
  border-radius: 50%;
  background-color: #ffd600;
  background-image: url("data:image/png;base64,");
  background-size: 80%;
  background-position: center center;
  background-repeat: no-repeat;
}
.cartLength{
  position: absolute;
  top: 0;
  right: 0;
  background-color: #f40;
  width: 36rpx;
  height: 36rpx;
  text-align: center;
  line-height: 36rpx;
  color: #fff;
  border-radius: 50%;
  z-index: 5;
  font-size: 20rpx;
}