/* pages/cart/cart.wxss */
.notLogin{
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  background-color: #efefef;
}
.toLogin{
  position: absolute;
  top: 42%;
  left: 32%;
  color: #e64f1a;
  border: 1px solid #e64f1a;
  padding: 20rpx 28rpx;
  border-radius: 20rpx;
  font-size: 36rpx;
}
.main{
  top: 0;
}
.fixView{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 540rpx;
  z-index: 5;
}
.addr_table{
  background-color: #fff;
  color: #333;
  font-size: 28rpx;
  font-weight: 500;
  background-image: url("data:image/png;base64,"),url("data:image/png;base64,");
  background-position: left top, left bottom;
  background-repeat: repeat-x;
  background-size: auto 6rpx;
  margin-bottom: 20rpx;
}
.tr{
  width: 100%;
  line-height: 60rpx;
  height: 60rpx;
  position: relative;
}
.widthSameT{
  display: inline-block;
  width: 28%;
  text-align: center;
  white-space: nowrap;
  overflow: hidden; 
  text-overflow: ellipsis;
}
.updateAddr{
  width: 17%;
  display: inline-block;
  position: absolute;
  right: 0;
  background-image: url("data:image/png;base64,");
  background-position: 70rpx center;
  background-repeat: no-repeat;
  background-size: auto 20rpx;
}
.widthFive{
  display: inline-block;
  width: 55%;
  white-space: nowrap;
  overflow: hidden; 
  text-overflow: ellipsis;    
}
/*table end  */
/*cart_body start  */
.cart_body{
  background-color: #fff;
}
.add_item{
  padding-top: 10rpx;
  color: #999;
  font-size: 22rpx;
  line-height: 40rpx;
  padding-left: 20rpx;
  border-bottom: 1px solid #e0e0e0;
}
.toCart{
  position: relative;
  font-size: 24rpx;
  height: 60rpx;
}
.cartName{
  padding-left: 10rpx;
  border-left: 4px solid #ffd600;
}
.btnToCart{
  position: absolute;
  right: 28rpx;
  display: inline-block;
  width: 140rpx;
  height: 48rpx;
  line-height: 48rpx;
  color: #ff3800;
  border: 2px solid #ff3800;
  top: 0;
  text-align: center;
  border-radius: 24rpx;
}

/*收货时间  */
.get_time{
  padding: 40rpx 20rpx 40rpx 20rpx;
  border-bottom: 1px solid #e0e0e0;
  line-height: 24px; 
  font-size: 28rpx;
  position: relative;
}
.get{
  margin-right: 50rpx;
}
picker{
  display: inline-block;
  position: absolute;
  right: 20rpx;
  left: 180rpx;
}
.picker{
  display: inline-block;
  color: #e64f1a;
}
.bookAble{
  position: absolute;
  right: 0;
  display: inline-block;
  width: 100rpx;
  background-image: url("data:image/png;base64,");
  background-position: 90rpx center;
  background-repeat: no-repeat;
  background-size: auto 20rpx;
}
/*收货备注  */
.remark_detail {
  display: inline-block;
  position: absolute;
  right: 20rpx;
  left: 180rpx;
  height: 62rpx;
  top: 32rpx;
  line-height: 62rpx;
  border: 1px solid #e0e0e0;
  border-radius: 10rpx;
}
.remark_detail input{
  height: 62rpx;
  margin-left: 6rpx;
}
/*cartsList   */
.cartsList{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 100rpx; 
  top: 572rpx;
}
.cartsListInner{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  overflow-y: scroll;
}
.carts_item{
  background-color: #fff;
  line-height: 80rpx;
  display: table;
  overflow: hidden;
  position: relative;
  width: 100%;
  border-bottom: 1px solid #e0e0e0;
}
.carts_item:last-child{
  margin-bottom: 80rpx;
}
.checkBox{
  display: table-cell;
  width: 100rpx;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-size: 36rpx 36rpx;
  background-repeat: no-repeat;
  height: 36rpx;
}
.checkBox.selected{
  background-image: url("data:image/png;base64,");
}
.carts_item .cartImg{
  width: 100rpx;
  height: 100rpx;
  display: table-cell;
  vertical-align: middle;
  padding-top: 28rpx;
}
.cartImg image{
  width: 100rpx;
  height: 100rpx;
}
.cartDetail{
  float: left;
  margin-left: 28rpx;
  line-height: 80rpx;
  font-size: 28rpx;
}
.btnArr{
  position: absolute;
  width: 200rpx;
  height: 100rpx;
  right: 0;
  bottom: 0;
  line-height: 100rpx;
  text-align: center;
}
.btnArr .subCartNum,.addCartNum{
  display: inline-block;
  line-height: 52rpx;
  width: 52rpx;
  height: 52rpx;
  color: #f40;
  font-weight: 700;
  border: 1px solid #f40;
  border-radius: 26rpx;
  box-sizing: border-box;
}
.btnArr .cartNum{
  display: inline-block;
  width: 52rpx;
  height: 52rpx;
}
/*count  */
.count{
  border-top: 1px solid #e0e0e0;
  height: 100rpx;
  line-height: 100rpx;
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
  background-color: #fff;
  font-size: 28rpx;
  font-weight: 400;
}
.count .allChecked{
  display: inline-block;
  position: relative;
  top: 10rpx;
}
.totalUp{
  margin-left: 80rpx;
  vertical-align: middle;
}
.total{
  color: #e64f1a;
}
.checkOut{
  position: absolute;
  right: 0;
  top: 0;
  bottom: 0;
  width: 200rpx;
  background-color: #ffd600;
  text-align: center;
  line-height: 100rpx;
}
/*cart_body end  */