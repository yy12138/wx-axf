/* pages/mine/mine.wxss */
.main{
  top: 0;
}
.mineHeader{
  background-color: #ffd600;
  height: 270rpx;
}
.toLoginIn,.LoginIn{
  height: 200rpx;
  font-size: 30rpx;
  color: #fff;
  font-weight: 700;
  position: relative;
  box-sizing: border-box;
}
.collect{
  height: 70rpx;
  font-size: 24rpx;
  background-color: rgba(0, 0, 0, 0.03);
  position: relative;
  color: #fff;
}
.productCollect,.storeCollect{
  width: 50%;
  display: inline-block;
  line-height: 70rpx;
  text-align: center;
  box-sizing: border-box;
}
.productCollect::after{
  content: '';
  display: inline-block;
  position: absolute;
  left: 50%;
  width: 1px;
  top: 17.5rpx;
  height: 35rpx;
  background-color: #fff;
}
.productCollect > text,.storeCollect > text{
  padding-left: 60rpx;
  background-repeat: no-repeat;
  background-size: auto 28rpx;
  background-position: left center;
}
.productCollect > text{
  background-image: url("data:image/png;base64,");
}
.storeCollect > text{
  background-image: url("data:image/png;base64,");
}
.toLoginIn{
  line-height: 200rpx;
  text-align: center;
}
.toLoginInPortrait{
  position: absolute;
  top: 50rpx;
  left: 35%;
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.toLoginInbtn{
  color: #fff;
  position: absolute;
  left: 46%;
  right: 34%;
  text-align: center;
  top: 35%;
  bottom: 35%;
  line-height: 60rpx;
}
